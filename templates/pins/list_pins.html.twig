{% extends 'base.html.twig' %}

{% block title 'Panterest'%}

{% block body %}
    {# <h1>{{ pluralize(pins|length, "Pin", "Pins") }}</h1> #}
    <h1>{{ pluralize(pins|length, "Pin") }}</h1>
    {% for pin in pins %}
        <article>
            <h2>{{ pin.title }}</h2>
            <p>{{ pin.description }}</p>
        </article>
    {% endfor %}
{% endblock %}
